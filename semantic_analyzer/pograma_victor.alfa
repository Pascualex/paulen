// Test operaciones aritméticas y lógicas

main {
  int x, y;
  boolean a, b;

  x = 4;
  y = 2;

  printf x + y; // 6
  printf x - y; // 2
  printf x / y; // 2
  printf x * y; // 8
  printf -x; // -4

  printf x * y - x / y; // 6
  printf x * (y - x) / y; // -4

  a = true;
  b = false;
  printf a && b; // false
  printf a || b; // true
  printf !a; // false

  printf a || b && !a; // false
  printf a || (b && !a); // true
}